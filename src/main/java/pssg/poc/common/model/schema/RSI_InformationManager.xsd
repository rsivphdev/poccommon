         <xsd:element maxOccurs="1" minOccurs="0" name="EntityTypePerson" nillable="false">
            <xsd:simpleType>
               <xsd:restriction base="tns:bool_y_n">
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
-->
         <xsd:element maxOccurs="1" minOccurs="1" name="COSFormNumber" nillable="false">
            <xsd:simpleType>
               <xsd:restriction base="tns:string_FreeFormText">
                  <xsd:maxLength value="50"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
