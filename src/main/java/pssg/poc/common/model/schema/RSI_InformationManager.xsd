         <xsd:element maxOccurs="1" minOccurs="1" name="ServedTo" nillable="false">
            <xsd:simpleType>
               <xsd:restriction base="tns:string_FreeFormText">
                  <xsd:maxLength value="100"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element maxOccurs="1" minOccurs="1" name="DateOfService" nillable="false" type="xsd:date"/>
